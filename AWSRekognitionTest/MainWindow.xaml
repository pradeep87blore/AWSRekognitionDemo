        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AWSRekognitionTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Initialized="Window_Initialized">
    <Grid>
        <Image x:Name="image_selectedImage" HorizontalAlignment="Left" Height="353" Margin="10,56,0,0" VerticalAlignment="Top" Width="390"/>
